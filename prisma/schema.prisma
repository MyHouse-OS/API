// This is your Prisma schema file for Prisma 7 with Bun optimization
// Learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider   = "prisma-client"
  output     = "./generated"
  engineType = "client"
  runtime    = "bun"
}

datasource db {
  provider = "postgresql"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

model Client {
  id          Int    @id @default(autoincrement())
  ClientID    String @unique
  ClientToken String
}
